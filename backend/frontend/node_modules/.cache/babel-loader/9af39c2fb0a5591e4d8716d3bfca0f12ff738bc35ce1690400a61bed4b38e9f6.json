{"ast":null,"code":"import { RouterLink } from 'vue-router';\nimport Logo from '@/components/Logo.vue';\nimport axios from 'axios'; // Import Axios for making HTTP requests\n\nexport default {\n  name: 'Create',\n  components: {\n    Logo\n  },\n  data() {\n    return {\n      username: '',\n      email: '',\n      password: '',\n      confirmPassword: ''\n    };\n  },\n  methods: {\n    createAccount() {\n      // Prepare data for registration request\n      const registrationData = {\n        username: this.username,\n        password: this.password\n        // Add more fields as needed (e.g., email, confirmPassword)\n      };\n\n      // Make a POST request to your CI4 backend API\n      axios.post('/register', registrationData).then(response => {\n        // Handle response from the backend\n        if (response.data.msg === 'okay') {\n          // Registration successful, handle accordingly (e.g., redirect to login)\n          console.log('Registration successful!');\n          // Redirect or show success message to the user\n        } else {\n          // Registration failed, handle accordingly (e.g., show error message)\n          console.log('Registration failed!');\n          // Show error message to the user\n        }\n      }).catch(error => {\n        // Handle errors if the request fails\n        console.error('Error during registration:', error);\n        // Show an error message to the user\n      });\n\n      // Reset form fields after submission\n      this.username = '';\n      this.email = '';\n      this.password = '';\n      this.confirmPassword = '';\n    }\n  }\n};","map":{"version":3,"names":["RouterLink","Logo","axios","name","components","data","username","email","password","confirmPassword","methods","createAccount","registrationData","post","then","response","msg","console","log","catch","error"],"sources":["C:\\laragon\\www\\Final\\App_Devs-PROJECT\\frontend\\src\\views\\Create.vue"],"sourcesContent":["<template>\r\n  <Logo />\r\n    <v-container>\r\n      <v-form @submit.prevent=\"createAccount\">\r\n        <v-row>\r\n          <v-col cols=\"12\">\r\n            <v-text-field v-model=\"username\" label=\"Username\" required></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\">\r\n            <v-text-field v-model=\"email\" label=\"Email\" type=\"email\" required></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\">\r\n            <v-text-field v-model=\"password\" label=\"Password\" type=\"password\" required></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\">\r\n            <v-text-field v-model=\"confirmPassword\" label=\"Confirm Password\" type=\"password\" required></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\">\r\n            <v-btn type=\"submit\" color=\"primary\" block class=\"mt-2\">Create Account</v-btn>\r\n          </v-col>\r\n          <v-col cols=\"12\">\r\n            <v-divider></v-divider>\r\n          </v-col>\r\n          <v-col cols=\"12\" class=\"text-center\">\r\n            <span>Already have an account? </span>\r\n            <router-link to=\"/\">\r\n            <v-btn color=\"secondary\" href=\"/\">Login</v-btn>\r\n          </router-link>\r\n          </v-col>\r\n        </v-row>\r\n      </v-form>\r\n    </v-container>\r\n  </template>\r\n  \r\n<script>\r\n  import { RouterLink } from 'vue-router';\r\n  import Logo from '@/components/Logo.vue';\r\n  import axios from 'axios'; // Import Axios for making HTTP requests\r\n  \r\n  export default {\r\n    name: 'Create',\r\n    components: {\r\n      Logo,\r\n    },\r\n    data() {\r\n      return {\r\n        username: '',\r\n        email: '',\r\n        password: '',\r\n        confirmPassword: '',\r\n      };\r\n    },\r\n    methods: {\r\n      createAccount() {\r\n        // Prepare data for registration request\r\n        const registrationData = {\r\n          username: this.username,\r\n          password: this.password,\r\n          // Add more fields as needed (e.g., email, confirmPassword)\r\n        };\r\n  \r\n        // Make a POST request to your CI4 backend API\r\n        axios.post('/register', registrationData)\r\n          .then(response => {\r\n            // Handle response from the backend\r\n            if (response.data.msg === 'okay') {\r\n              // Registration successful, handle accordingly (e.g., redirect to login)\r\n              console.log('Registration successful!');\r\n              // Redirect or show success message to the user\r\n            } else {\r\n              // Registration failed, handle accordingly (e.g., show error message)\r\n              console.log('Registration failed!');\r\n              // Show error message to the user\r\n            }\r\n          })\r\n          .catch(error => {\r\n            // Handle errors if the request fails\r\n            console.error('Error during registration:', error);\r\n            // Show an error message to the user\r\n          });\r\n  \r\n        // Reset form fields after submission\r\n        this.username = '';\r\n        this.email = '';\r\n        this.password = '';\r\n        this.confirmPassword = '';\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* Add your custom styles here */\r\n  </style>\r\n  "],"mappings":"AAmCE,SAASA,UAAS,QAAS,YAAY;AACvC,OAAOC,IAAG,MAAO,uBAAuB;AACxC,OAAOC,KAAI,MAAO,OAAO,EAAE;;AAE3B,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,aAAaA,CAAA,EAAG;MACd;MACA,MAAMC,gBAAe,GAAI;QACvBN,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBE,QAAQ,EAAE,IAAI,CAACA;QACf;MACF,CAAC;;MAED;MACAN,KAAK,CAACW,IAAI,CAAC,WAAW,EAAED,gBAAgB,EACrCE,IAAI,CAACC,QAAO,IAAK;QAChB;QACA,IAAIA,QAAQ,CAACV,IAAI,CAACW,GAAE,KAAM,MAAM,EAAE;UAChC;UACAC,OAAO,CAACC,GAAG,CAAC,0BAA0B,CAAC;UACvC;QACF,OAAO;UACL;UACAD,OAAO,CAACC,GAAG,CAAC,sBAAsB,CAAC;U